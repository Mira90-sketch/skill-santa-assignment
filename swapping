# creating list
list = []

#number of element in list
num = int(input("Enter number of elements in list: "))

# iterating till num to append elements in list
for i in range(1, num + 1):
    ele = int(input("Enter elements: "))
    list.append(ele)


#swap function
def swapList(list):
    size = len(list)

    # Swapping
    temp = list[0]
    list[0] = list[size - 1]
    list[size - 1] = temp

    return list


# printing the output

print(list)
print(swapList(list))
